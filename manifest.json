{
  "manifest_version": 2,
  "name": "YouTube Shorts Limiter",
  "version": "1.0",
  "description": "Warns you after scrolling 10 YouTube Shorts",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}